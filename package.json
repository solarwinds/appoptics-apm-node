{
  "name": "traceview",
  "version": "1.0.2",
  "description": "Agent for AppNeta TraceView",
  "main": "index.js",
  "bin": {
    "node-tv": "./bin/node-tv"
  },
  "scripts": {
    "test": "mocha",
    "docs": "yuidoc .",
    "install": "./build.sh",
    "coverage": "npm run coverage:report",
    "coverage:build": "rm -rf coverage && istanbul cover _mocha",
