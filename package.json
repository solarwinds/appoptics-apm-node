  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appneta/node-traceview"
  },
  "bugs": {
    "url": "https://github.com/appneta/node-traceview/issues"
  },
  "gypfile": true,
  "dependencies": {
    "bindings": "~1.2.0",
